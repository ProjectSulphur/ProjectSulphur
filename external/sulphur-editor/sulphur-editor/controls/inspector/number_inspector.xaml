<UserControl x:Class="sulphur.editor.controls.inspector.NumberInspector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="300">
    <Grid>
        <TextBox x:Name="TextBox1" Text="0.5" VerticalAlignment="Center" Height="16" Margin="5,7" Padding="0,-1,0,0" VerticalContentAlignment="Center" PreviewTextInput="TextBox1_PreviewTextInput"  DataObject.Pasting="TextBox1_Pasting" LostFocus="TextBox1_LostFocus" KeyDown="TextBox1_KeyDown"/>
    </Grid>
</UserControl>
